{
  "name": "dvulndb",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "install:safe": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 yarn install",
    "test": "pwsh anchor/scripts/test.ps1",
    "test:anchor": "anchor test",
    "test:unit": "anchor test --skip-deploy --skip-local-validator || echo 'Unit tests skipped - contracts not built yet'",
    "test:build": "anchor build && anchor test --skip-local-validator",
    "test:framework": "pwsh -Command \"$env:ANCHOR_PROVIDER_URL='http://localhost:8899'; $env:ANCHOR_WALLET='$env:USERPROFILE\\.config\\solana\\id.json'; npx ts-mocha anchor/tests/framework.ts --timeout 60000\"",
    "deploy:devnet": "pwsh anchor/scripts/deploy-devnet.ps1",
    "deploy:testnet": "pwsh anchor/scripts/deploy-testnet.ps1",
    "deploy:mainnet": "pwsh anchor/scripts/deploy-mainnet.ps1",
    "build:anchor": "anchor build",
    "deploy:local": "anchor deploy",
    "build:check": "node scripts/build-check.js",
    "prebuild": "yarn build:check",
    "verify:devnet": "node scripts/verify-deployment.js devnet",
    "verify:testnet": "node scripts/verify-deployment.js testnet",
    "verify:mainnet": "node scripts/verify-deployment.js mainnet"
  },
  "dependencies": {
    "@next/font": "^14.2.15",
    "@protobufjs/aspromise": "^1.1.2",
    "@protobufjs/base64": "^1.1.2",
    "@protobufjs/codegen": "^2.0.4",
    "@protobufjs/eventemitter": "^1.1.0",
    "@protobufjs/fetch": "^1.1.0",
    "@protobufjs/float": "^1.0.2",
    "@protobufjs/inquire": "^1.1.0",
    "@protobufjs/path": "^1.1.2",
    "@protobufjs/pool": "^1.1.0",
    "@protobufjs/utf8": "^1.1.0",
    "@reown/appkit": "^1.7.9",
    "@reown/appkit-adapter-solana": "^1.7.9",
    "@solana/web3.js": "^1.95.2",
    "@tanstack/react-query": "^5.80.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "cross-env": "^10.0.0",
    "framer-motion": "^12.16.0",
    "helia": "^5.4.2",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^11.1.0",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.31.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "chai": "^4.3.10",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "mocha": "^10.2.0",
    "pino-pretty": "^13.0.0",
    "postcss-nesting": "^13.0.2",
    "tailwindcss": "^4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
